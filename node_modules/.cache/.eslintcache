);\r\n\r\n    // פונקציה לטיפול בהעלאת תמונות\r\n    const handleImageUpload = (e) => {\r\n        setIsLoading(true); // מתחילים טעינה\r\n        const files = Array.from(e.target.files);\r\n        const newImages = files.map(file => URL.createObjectURL(file));\r\n\r\n        // סימולציה של זמן טעינה (למשל בזמן העלאה לשרת)\r\n        setTimeout(() => {\r\n            setImages(prevImages => [...prevImages, ...newImages]);\r\n            setIsLoading(false); // סיום טעינה\r\n        }, 2000); // סימולציה של 2 שניות טעינה\r\n    };\r\n\r\n    // פונקציה לעמוד הבא\r\n    const handleNextPage = () => {\r\n        if (currentPage < totalPages) {\r\n            setIsLoading(true); // מתחילים טעינה\r\n            setTimeout(() => {\r\n                setCurrentPage(currentPage + 1);\r\n                setIsLoading(false); // סיום טעינה\r\n            }, 1000); // סימולציה של טעינה של דף\r\n        }\r\n    };\r\n\r\n    // פונקציה לעמוד הקודם\r\n    const handlePrevPage = () => {\r\n        if (currentPage > 1) {\r\n            setIsLoading(true); // מתחילים טעינה\r\n            setTimeout(() => {\r\n                setCurrentPage(currentPage - 1);\r\n                setIsLoading(false); // סיום טעינה\r\n            }, 1000); // סימולציה של טעינה של דף\r\n        }\r\n    };\r\n\r\n    // מחיקת אירוע\r\n    const handleDeleteEvent = () => {\r\n        if (window.confirm(`האם אתה בטוח שברצונך למחוק את האירוע של ${event.name}?`)) {\r\n            navigate('/profile');\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            <h2 className=\"Name-style\">פרטי האירוע</h2>\r\n            {event ? (\r\n                <div>\r\n                    <p>שם האירוע: {event.name}</p>\r\n                    <p>מיקום: {event.location}</p>\r\n                    <p>תאריך: {event.date}</p>\r\n\r\n                    <div>\r\n                        <h3>הוסף תמונות</h3>\r\n                        <input\r\n                            type=\"file\"\r\n                            accept=\"image/*\"\r\n                            multiple\r\n                            onChange={handleImageUpload}\r\n                        />\r\n                        {isLoading ? (\r\n                            <p>טוען...</p>\r\n                        ) : (\r\n                            <>\r\n                                {images.length > 0 && (\r\n                                    <div>\r\n                                        <p>{`מוצגות ${currentImages.length} תמונות מתוך ${images.length} סה\"כ`}</p>\r\n                                        <div className=\"image-gallery\">\r\n                                            {currentImages.map((img, index) => (\r\n                                                <img key={index} src={img} alt={`Event ${index}`} className=\"event-image\" />\r\n                                            ))}\r\n                                        </div>\r\n                                    </div>\r\n                                )}\r\n\r\n                                {/* כפתורי Pagination */}\r\n                                {images.length > imagesPerPage && (\r\n                                    <div className=\"pagination\">\r\n                                        <button onClick={handlePrevPage} disabled={currentPage === 1}>\r\n                                            קודם\r\n                                        </button>\r\n                                        <span>עמוד {currentPage} מתוך {totalPages}</span>\r\n                                        <button onClick={handleNextPage} disabled={currentPage === totalPages}>\r\n                                            הבא\r\n                                        </button>\r\n                                    </div>\r\n                                )}\r\n                            </>\r\n                        )}\r\n                    </div>\r\n\r\n                    <button onClick={handleDeleteEvent} className=\"delete-button\">מחק אירוע</button>\r\n                    <Link className=\"Link-exit\" to=\"/profile\">חזור לפרופיל</Link>\r\n                </div>\r\n            ) : (\r\n                <p>לא נמצאו פרטי אירוע.</p>\r\n            )}\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default EventPage;\r\n","C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Register.js",["77"],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Profile.js",[],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Packages.js",["78","79"],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Home.js",["80","81"],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\PurchasePackages.js",[],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\AboutUs.js",["82","83"],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Navbar.js",[],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Carousel.js",[],[],"C:\\Users\\DELL\\WebstormProjects\\newAiuaPhotographer\\src\\components\\Block.js",[],[],{"ruleId":"84","severity":1,"message":"85","line":2,"column":10,"nodeType":"86","messageId":"87","endLine":2,"endColumn":14},{"ruleId":"84","severity":1,"message":"88","line":6,"column":8,"nodeType":"86","messageId":"87","endLine":6,"endColumn":14},{"ruleId":"84","severity":1,"message":"89","line":1,"column":27,"nodeType":"86","messageId":"87","endLine":1,"endColumn":36},{"ruleId":"90","severity":1,"message":"91","line":17,"column":8,"nodeType":"92","endLine":17,"endColumn":65,"suggestions":"93"},{"ruleId":"84","severity":1,"message":"85","line":3,"column":23,"nodeType":"86","messageId":"87","endLine":3,"endColumn":27},{"ruleId":"84","severity":1,"message":"88","line":5,"column":8,"nodeType":"86","messageId":"87","endLine":5,"endColumn":14},{"ruleId":"84","severity":1,"message":"85","line":2,"column":10,"nodeType":"86","m